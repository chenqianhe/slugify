{
  "name": "url-slug-generator",
  "version": "1.0.0",
  "description": "Format string to slug",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenqianhe/slugify.git"
  },
  "main": "dist/cjs/slugify.js",
  "module": "dist/esm/slugify.js",
  "types": "dist/esm/slugify.d.ts",
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "publish": "npm run build && npm publish"
  },
  "keywords": [
    "url",
    "slug"
  ],
  "author": "Qianhe Chen",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
